{
    "eslint.enable": true,
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "javascript.validate.enable": true,
    "editor.formatOnSave": true,
    "files.associations": {
      "*.js": "javascript",
      "*.jsx": "javascript",
      "*.ts": "typescriptreact",
      "*.tsx": "typescriptreact"
    },
    "[javascript]": {
      "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    }
  }
